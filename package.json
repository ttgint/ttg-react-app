{
  "name": "ttg-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@craco/craco": "^3.3.1",
    "antd": "^3.19.1",
    "axios": "^0.18.0",
    "craco-antd": "^1.10.0",
    "craco-less": "^1.8.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "react-scripts": "^2.1.5",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.2.0"
  },
  "devDependencies": {
    "@ttgint/eslint-config-react": "1.x",
    "eslint": "5.12.0",
    "husky": "^2.3.0",
    "lint-staged": "^8.1.7",
    "prettier": "^1.17.1"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test --no-watch",
